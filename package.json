  "bugs": {
    "url": "https://github.com/ramsunvtech/react-camouflage/issues"
  },
  "homepage": "https://github.com/ramsunvtech/react-camouflage#readme",
  "dependencies": {
    "prop-types": "^15.8.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "react": "^17.0.2",
